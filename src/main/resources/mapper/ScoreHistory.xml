<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cykj.demo.mapper.ScoreHistoryMapper">

    <insert id="add" parameterType="com.cykj.demo.entity.ScoreHistory">
		insert into tb_score_history (user_id,type,score)
		values( #{userId},#{type},#{score})
	</insert>

   
</mapper>
	